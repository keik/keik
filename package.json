{
  "name": "sandbox-react-ui-lib-with-ts",
  "version": "1.0.3",
  "author": "keik <keik@c-fo.com>",
  "dependencies": {
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.0.1",
    "babel-plugin-styled-components": "^1.10.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "license": "MIT",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "repository": "https://github.com/keik/sandbox-react-ui-lib-with-ts",
  "scripts": {
    "build": "run-p build:*",
    "build:cjs": "tsc --outDir lib/cjs && babel lib/cjs --config-file ./babel.config.cjs.js -d lib/cjs && rimraf 'lib/cjs/*.jsx'",
    "build:decl": "tsc -p tsconfig.decl.json --outDir lib/types",
    "build:esm": "tsc --outDir lib/esm && babel lib/esm --config-file ./babel.config.esm.js -d lib/esm && rimraf 'lib/esm/*.jsx'",
    "clean": "rimraf lib"
  },
  "sideEffects": false,
  "types": "./lib/types/index.d.ts"
}
